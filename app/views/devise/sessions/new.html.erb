<div  class="animated  fadeInRightBig "  style="background: url('/assets/c.jpg'); background-size: cover;background-position: center;">
  <div class="animated  fadeInRightBig">
    <div class="panel-heading ">
      <h1 style="text-align: center;color: white">Log in</h1>
    </div>
    <div class="panel-body">
      <div class="col-md-6" style="margin-left: 30%">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="field form-group">
          <%= f.label :email, :class => 'control-label', :style =>'color:white;font-size:15px' %><br />
          <%= f.email_field :email, autofocus: true, :class => 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :password, :class => 'control-label ', :style =>'color:white;font-size:15px' %><br />
          <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %>
        </div>
        <% if devise_mapping.rememberable? -%>
        <div class="field">
          <%= f.check_box :remember_me, :style =>'color:white' %>
          <%= f.label :remember_me, :style =>'color:white;font-size:15px' %>
        </div>
        <% end -%>
        <div class="actions form-group">
          <%= f.submit "Log in", :class => 'form-control', :style =>'color:white' %>
        </div>
        <% end %>
        <%= render "devise/shared/links", :style =>'color:white;font-size:15px' %>
      </div>
    </div>
  </div>
</div>